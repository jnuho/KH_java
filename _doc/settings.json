{
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.configuration.checkProjectSettingsExclusions": false,
    "terminal.integrated.fontSize" : 13,
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "code-runner.saveAllFilesBeforeRun": true,
    "code-runner.saveFileBeforeRun": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "git.decorations.enabled": true,
    "explorer.decorations.colors": false,
    "explorer.decorations.badges": false,
    "window.zoomLevel": -1,
    "editor.suggestSelection": "first",
    "editor.wordWrap" : "on",
    "editor.fontFamily": "Consolas",
    "editor.fontSize": 15,
    "editor.fontLigatures": true,
    "editor.tabSize": 4,
    "java.home": "C:\\Program Files\\Java\\jdk1.8.0_211",
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "j"],
            "after": ["<esc>"]
        },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["z", "z"],
            "commands": [   
                ":w"
            ]
        },
        {
            "before":["<C-n>"],
            "commands": [":nohl"],
        },
        {
            "before": ["leader", "w"],
            "commands": [
                "workbench.action.files.save",
            ]
        },
        {
            "before": ["<C-b>"],
            "commands": [
                "workbench.action.focusSideBar",
            ]
        },
        // {
        //     "before": ["<leader>", "d"],
        //     "after": ["d", "d"]
        // },
    ],
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorCustomizations": {
        "editorIndentGuide.activeBackground": "#108777",
        "editorIndentGuide.background": "#1D4F0F",
        "editorWhitespace.foreground": "#1D4F0F",

        "[MagicWB_Amiga]": {
            "editor.background": "#B1B4B1",
            "sideBar.background": "#343434",
            "sideBar.foreground": "#ABADAB",
            "terminal.background": "#484848",
            "terminal.foreground": "#CBCBCB",
        },
    },
    "[html]": { "editor.tabSize": 2 },
    "[css]": { "editor.tabSize": 2 },
    "[javascript]": { "editor.tabSize": 2 },
    "[json]": { "editor.tabSize": 2 },
    "[plaintext]": { "editor.tabSize": 2 },
    "editor.insertSpaces": true,
    "editor.tokenColorCustomizations": {
        "[DuoTone Dark Forest]": {
            "textMateRules": [
                {
                    // "scope": "entity.name.tag",
                    "scope": "comment",
                    "settings": {
                        // "foreground": "#ddf8dd",
                        "foreground": "#6C7D6C",
                    }
                },
                {
                    "scope": "punctuation",
                    "settings": {
                        "foreground": "#6C7D6C",
                    }
                },
                {
                    "scope": "meta.brace",
                    "settings": {
                        "foreground": "#6C7D6C",
                    }
                },
            ],
  
        },
        "[MagicWB_Amiga]": {
             "strings": "#DFDFDF",
             // JAVA
            "textMateRules": [
                // constant.character { "background": "#0000FF33", "fontStyle": "", "foreground": "#FFA995" }
                {
                    "scope": "keyword",
                    "settings": {
                        "fontStyle": "",
                        "foreground": "#000fff",
                    },
                },
                {
                    "scope": "storage",
                    "settings": {
                        "fontStyle": "",
                        "foreground": "#000000",
                    },
                },
                {
                    // entity.name.function { "fontStyle": "", "foreground": "#FFA995" }
                    "scope": "entity.name.function",
                    "settings": {
                        "fontStyle": "bold underline",
                        "foreground": "#000fff",
                    }

                },
                {
                    // variable.other { "foreground": "#FFA995" }
                    "scope": "variable.other",
                    "settings": {
                        "fontStyle": "italic",
                        "foreground": "#000000",
                    }

                },
                {
                    "scope": "string",
                    "settings": {
                        "fontStyle": "italic",
                        "foreground": "#0A52BB",
                    }
                    
                },
                {
                    "scope": "comment",
                    "settings": {
                        "fontStyle": "",
                        "foreground": "#A2047B",
                    }
                },
                {
                    "scope": "constant.numeric",
                    "settings": {
                        "fontStyle": "",
                        "foreground": "#334031",
                    }
                },

                // HTML
                {
                    "scope": "entity.other.attribute-name",
                    "settings": {
                        "fontStyle": "bold",
                        "foreground": "#0856BC"
                    }
                },
                // settings.json
                {
                    "scope": "support.type",
                    "settings": {
                        // "foreground": "#E7F6DA",
                        "foreground": "#000000",
                    },
                }
            ]
        }
    },
    "vim.handleKeys": {
        "<C-w>": false,
        "<C-f>": false,
        "<C-n>": false,
        "<C-s>": false,
    },
    "terminal.integrated.fontFamily": "monospace",
    "workbench.colorTheme": "Monokai Grey",
    "editor.renderWhitespace": "all",
}